import pyautogui, time
r = input("Please enter the required website: ")
if r.__contains__("www."):
    r = r
else:
    g = "www."+r+".com"

pyautogui.hotkey('winleft')
time.sleep(1.0)
pyautogui.typewrite('chrome\n', 0.5)
time.sleep(1.0)
if r == r:
    pyautogui.typewrite(r, 0.5)
else:
    pyautogui.typewrite(g,0.5)
pyautogui.hotkey('enter')
time.sleep(1.0)

##simple Script by Ashishsecdev
