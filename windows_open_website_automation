import pyautogui
import time # required to set time to execute next task.
r = input("Please enter the required website: ") # Enter the name of website to open, it will only work for .com TLD
g = "www."+r+".com" #
pyautogui.hotkey('winleft') # use 'win' or 'winleft'
time.sleep(1.0)
pyautogui.typewrite('chrome\n', 0.5) # open chrome
time.sleep(1.0)
pyautogui.typewrite(g,0.5)
pyautogui.hotkey('enter') # open the required website.

